public class Animal {

    private String adi;
    private String sinifi;



    public Animal(String adi,String sinifi){

        this.adi=adi;
        this.sinifi=sinifi;
    }

    public String getAdi() {
        return adi;
    }

    public void setAdi(String adi) {
        this.adi = adi;
    }

    public String getSinifi() {
        return sinifi;
    }

    public void setSinifi(String sinifi) {
        this.sinifi = sinifi;
    }



}
